# Frontend Supabase Configuration
# Copy this file to .env in the frontend directory (kys-frontend) and fill in your actual Supabase credentials

# Supabase Project URL (same as backend)
REACT_APP_SUPABASE_URL=https://your-project.supabase.co

# Supabase Anon Key (Public API Key - safe to use in frontend)
REACT_APP_SUPABASE_ANON_KEY=your-anon-key-here

# API URL for backend communication (existing)
REACT_APP_API_URL=http://localhost:5003/api

# For production, use your backend URL:
# REACT_APP_API_URL=https://your-backend-url.com/api
