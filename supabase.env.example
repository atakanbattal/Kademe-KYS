# Supabase Configuration
# Copy this file to .env and fill in your actual Supabase credentials

# Supabase Project URL
SUPABASE_URL=https://your-project.supabase.co

# Supabase Anon Key (Public API Key)
SUPABASE_ANON_KEY=your-anon-key-here

# Supabase Service Role Key (Private API Key - only use on server-side)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# JWT Secret for local authentication
JWT_SECRET=your-jwt-secret-here

# Server Configuration
PORT=5003
NODE_ENV=development

# Legacy MongoDB Configuration (can be removed after migration)
MONGODB_URI=mongodb://localhost:27017/kys

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:3000
